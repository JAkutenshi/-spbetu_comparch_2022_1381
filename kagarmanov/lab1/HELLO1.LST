 0005  BA 0000 R		   mov  dx, OFFSET Greeting              ; Загрузка в dx смещения 
				                                        ; адреса текста привествия
 0008				DisplayGreeting:
 0008  B4 09			   mov  ah, 9                            ; # функции ДОС печати строки
 000A  CD 21			   int  21h                             ; выводна экран приветствия
 000C  B4 4C			   mov  ah, 4ch                          ; # функции ДОС завершенич программы
 000E  CD 21			   int  21h                             ; завершение программы и выход в ДОС
				   END
